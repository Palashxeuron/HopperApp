{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Start Dev",
			"type": "node",
			"request": "launch",
			"cwd": "${workspaceFolder}",
			"runtimeExecutable": "${workspaceFolder}/node_modules/.bin/electron",
			"windows": {
				"runtimeExecutable": "${workspaceFolder}/node_modules/.bin/electron.cmd"
			},
			"args": ["--trace-warnings", "."],
			"env": {
				"ENV": "dev",
				"STATE": "start"
			},
			"output,Capture": "std"
		},

	]
}